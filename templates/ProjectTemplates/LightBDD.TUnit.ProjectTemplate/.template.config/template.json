{
  "$schema": "http://json.schemastore.org/template",
  "author": "LightBDD",
  "classifications": [ "LightBDD", "TUnit" ],
  "identity": "LightBDD.TUnit.NetCore.ProjectTemplate",
  "name": "LightBDD.TUnit Project (CLI)",
  "description": "LightBDD.TUnit.Project",
  "shortName": "LightBDD.TUnit.Project",
  "sourceName": "$safeprojectname$",
  "tags": {
    "language": "C#",
    "type": "project"
  },
  "symbols": {
    "featurename": {
      "type": "parameter",
      "description": "The feature name of the file.",
      "datatype": "text",
      "replaces": "My_feature",
      "fileRename": "StringExtensions",
      "defaultValue": "FeatureTestClass"
    }
  }
}