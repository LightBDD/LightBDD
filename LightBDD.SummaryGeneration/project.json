{
  "version": "1.0.0-*",
  "buildOptions": {
    "compile": [ "**/*.cs", "../AssemblyVersion.cs" ],
    "embed": {
      "include": [
        "Formatters/Html/scripts.js",
        "Formatters/Html/styles.css"

      ],
      "mappings": {
        "LightBDD/SummaryGeneration/Formatters/Html/lightbdd_small.ico": "../logo/lightbdd_small.ico"
      }
    }
  },
  "frameworks": {
    "netstandard1.3": {
      "dependencies": { "NETStandard.Library": "1.6.0" }
    },
    "net45": {
      "frameworkAssemblies": {
        "System.Xml": "4.0.0.0",
        "System.Runtime": "4.0.0.0",
        "System.Xml.Linq": "4.0.0.0"
      }
    }
  },
  "dependencies": {
    "LightBDD.Core": {
      "target": "project"
    }
  }
}
